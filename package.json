{
  "name": "@thaunknown/simple-peer",
  "description": "Simple one-to-one WebRTC video/voice and data channels",
  "version": "10.0.11",
  "type": "module",
  "files": [
    "./*.js",
    "./*.js.map",
    "./*.d.ts",
    "./*.d.ts.map"
  ],
  "author": {
    "name": "ThaUnknown",
    "email": "casistaken@gmail.com",
    "url": "http://github.com/ThaUnknown"
  },
  "bugs": {
    "url": "https://github.com/thaunknown/simple-websocket/issues"
  },
  "exports": {
    ".": "./index.js",
    "./lite": "./lite.js",
    "./full": "./full.js",
    "./*": "./*.js"
  },
  "dependencies": {
    "debug": "^4.3.7",
    "err-code": "^3.0.1",
    "streamx": "^2.20.1",
    "uint8-util": "^2.2.5",
    "webrtc-polyfill": "^1.1.10"
  },
  "devDependencies": {
    "@babel/core": "^7.26.0",
    "@babel/preset-env": "^7.26.0",
    "@types/streamx": "^2.9.5",
    "airtap": "^4.0.4",
    "airtap-manual": "^1.0.0",
    "airtap-system": "^0.1.0",
    "babelify": "^10.0.0",
    "bowser": "^2.11.0",
    "cross-env": "^7.0.3",
    "esbuild": "^0.24.2",
    "prettier-bytes": "^1.0.4",
    "speedometer": "^1.1.0",
    "string-to-stream": "^3.0.1",
    "tap-spec": "^5.0.0",
    "tape": "^5.9.0",
    "thunky": "^1.1.0",
    "typescript": "^5.7.2",
    "ws": "^8.18.0"
  },
  "keywords": [
    "data",
    "data channel",
    "data channel stream",
    "data channels",
    "p2p",
    "peer",
    "peer",
    "peer-to-peer",
    "stream",
    "video",
    "voice",
    "webrtc",
    "webrtc stream"
  ],
  "license": "MIT",
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "git://github.com/thaunknown/simple-peer.git"
  },
  "scripts": {
    "build": "mkdir -p dist && rm -rf ./dist/* && esbuild ./src/*.ts --tsconfig=tsconfig.build.json --outdir=. && tsc --emitDeclarationOnly --project tsconfig.build.json --outDir dist",
    "test": "npm run test-node && npm run test-browser",
    "test-browser": "airtap --concurrency 1 --all -- test/*.js test/browser/*.js | tap-spec",
    "test-browser-local": "airtap --preset local -- test/*.js | tap-spec",
    "test-node": "npm run build && tape test/*.js",
    "test-node-debug": "cross-env DEBUG=* tape test/negotiation.js",
    "prepublishOnly": "npm run build"
  },
  "contributors": [
    {
      "name": "Nick Thomas",
      "email": "nichoth@nichoth.com",
      "url": "https://nichoth.com"
    },
    {
      "name": "Feross Aboukhadijeh",
      "email": "feross@feross.org",
      "url": "https://feross.org"
    }
  ]
}
